{
  "name": "vue-file-agent",
  "version": "1.7.3",
  "description": "High performant Vue file upload component with elegant and distinguishable previews for every file type and support for drag and drop, validations, default uploader with progress support and externally customizable in the \"vue way\"",
  "main": "dist/vue-file-agent.umd.js",
  "types": "types/src/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-file-agent ./src/index.ts",
    "upload-server": "node upload-server-examples/node/upload-server.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:watch": "vue-cli-service test:unit --watch --coverage",
    "build:dev": "vue-cli-service build --watch --target lib --name vue-file-agent ./src/index.ts",
    "docs:serve:dev": "jekyll serve --config _config.yml,_config_dev.yml --host 0.0.0.0",
    "docs:serve": "jekyll serve --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safrazik/vue-file-agent.git"
  },
  "keywords": [
    "vue",
    "file",
    "upload",
    "input",
    "progress",
    "drag",
    "drop"
  ],
  "author": "Safraz Razik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safrazik/vue-file-agent/issues"
  },
  "homepage": "https://github.com/safrazik/vue-file-agent#readme",
  "files": [
    "/src",
    "/dist",
    "/types"
  ],
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/tus-js-client": "^2.1.0",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "^2.0.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "tus-js-client": "^3.0.0-0",
    "typescript": "^4.7.4",
    "vue-slicksort": "^1.2.0",
    "vue-template-compiler": "^2.7.8"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
